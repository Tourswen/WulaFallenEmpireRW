							<name>skin</name>
              <first Class="AlienRace.ColorGenerator_SkinColorMelanin">
                <minMelanin>0</minMelanin>
                <maxMelanin>0</maxMelanin>
              </first>
						</li>
            <!-- 头发颜色 -->
						<li>
							<name>hair</name>
							<first Class="ColorGenerator_Options">
								<options>
									<li>
										<weight>15</weight>
										<min>(0.4,0.3,0.5)</min>
										<max>(0.6,0.1,0.7)</max>
									</li>
									<li>
										<weight>6</weight>
										<min>(0.9,0.9,0.9)</min>
										<max>(0.9,0.9,0.9)</max>
									</li>
									<li>
										<weight>6</weight>
										<min>(1,0.8,0.8)</min>
										<max>(1,0.9,0.9)</max>
									</li>
									<li>
										<weight>3</weight>
										<min>(1,1,1)</min>
										<max>(1,1,1)</max>
									</li>
								</options>
							</first>
						</li>
					</colorChannels>
